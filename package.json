    "@esmx/rspack-vue": "^3.0.0-rc.107",
    "@esmx/rspack-react": "^3.0.0-rc.107",
    "@esmx/router": "^3.0.0-rc.107",
    "@esmx/router-vue": "^3.0.0-rc.107",
    "@esmx/router-react": "^3.0.0-rc.107",
    "typescript": "^5.0.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0"
  },
  "workspaces": [
    "my-super-app/*"
  ]
